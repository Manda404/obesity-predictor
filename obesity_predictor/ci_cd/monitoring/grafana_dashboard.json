{
  "id": null,
  "title": "ðŸ“Š Obesity Predictor Monitoring Dashboard",
  "tags": ["mlops", "fastapi", "mlflow", "streamlit"],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "panels": [
    {
      "type": "graph",
      "title": "API Request Rate (req/sec)",
      "targets": [
        {
          "expr": "rate(http_requests_total[1m])",
          "legendFormat": "Requests",
          "refId": "A"
        }
      ],
      "yaxes": [
        { "format": "reqps", "label": "Requests/s" },
        { "format": "short" }
      ]
    },
    {
      "type": "graph",
      "title": "API Response Time (p95)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95 latency",
          "refId": "B"
        }
      ],
      "yaxes": [
        { "format": "s", "label": "Seconds" },
        { "format": "short" }
      ]
    },
    {
      "type": "stat",
      "title": "Total Predictions Served",
      "targets": [
        {
          "expr": "sum(http_requests_total{path=\"/api/v1/predict/\"})",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1000 },
              { "color": "red", "value": 5000 }
            ]
          }
        }
      }
    },
    {
      "type": "graph",
      "title": "CPU Usage (%)",
      "targets": [
        {
          "expr": "avg(rate(process_cpu_seconds_total[1m])) * 100",
          "legendFormat": "CPU %",
          "refId": "D"
        }
      ],
      "yaxes": [
        { "format": "percent", "label": "CPU %" },
        { "format": "short" }
      ]
    },
    {
      "type": "graph",
      "title": "Memory Usage (MB)",
      "targets": [
        {
          "expr": "process_resident_memory_bytes / 1024 / 1024",
          "legendFormat": "Memory (MB)",
          "refId": "E"
        }
      ],
      "yaxes": [
        { "format": "megabytes", "label": "Memory" },
        { "format": "short" }
      ]
    }
  ],
  "templating": { "list": [] },
  "refresh": "10s"
}
